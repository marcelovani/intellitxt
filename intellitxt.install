<?php

/*
 * Implements hook_install().
 * Make this module heavy as the JS needs to be added last.
 */
function intellitxt_install() {
  db_update('system')
    ->fields(array('weight' => 99))
    ->condition('name', 'intellitxt', '=')
    ->execute();	
}

